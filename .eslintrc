{
    "extends": "airbnb",
    "rules": {
        "quotes": ["warn", "double"],
        "no-plusplus": "off",
        "class-methods-use-this": "off",
        "no-continue":"off",
        "import/no-extraneous-dependencies": "off",
        "import/resolver": "off"
    },
    "settings": {
        "import/resolver": "babel-plugin-root-import"
    }
}